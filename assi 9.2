mysql> create FUNCTION PriceInd2(Price int) returns varchar(20)
    -> deterministic
    -> begin
    -> declare Price_Ind varchar(20);
    -> if Price<=70 then set Price_Ind='LOW';
    -> elseif(Price>70 and Price<=90) then set Price_Ind='MEDIUM';
    -> elseif Price>90 then set Price_Ind='HIGH';
    -> End if;
    -> return(Price_Ind);
    -> end
